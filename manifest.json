{

	"manifest_version": 2,
	"name": "Nort Crypt",
	"version": "1.0",

	"description": "Mix letters in words",
	"icons": {
		"48": "icons/icon-48.png",
		"32": "icons/icon-32.png"
	},

	"applications": {
		"gecko": {
			"id": "nortcrypt@vlad.prekel",
			"strict_min_version": "45.0"
		}
	},

	"content_scripts": [
		{
			"matches": [ "*://*.*.*/*" ],
